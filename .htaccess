RewriteRule ^articles/([0-9]+)$ articles.php?page=$1 [NC,L] 
RewriteRule ^article-detail/([0-9]+)/(.*)$ article-detail.php?id=$1 [NC,L]     
RewriteRule ^services/([0-9]+)/(.*)$ services.php?cat=$1 [NC,L]   
